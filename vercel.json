{
  "version": 2,
  "rewrites": [
    {
      "source": "/((?!api|assets|favicon|robots|site\\.webmanifest|browserconfig|_next|.*\\.).*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        }
      ]
    }
  ]
}
